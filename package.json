    "storybook": "8.3.0-alpha.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^2.0.0",
    "wrangler": "^3.78.12"
  },
  "type": "module",
  "dependencies": {
    "@corbado/node-sdk": "^3.0.2",
    "@corbado/web-js": "^2.14.0"
